{
  "name": "@ibootcloud/js-sdk",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/js-sdk.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/js-sdk.mjs"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/js-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/js-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "author": {
    "name": "lrvinye",
    "email": "lrvinye@qq.com"
  },
  "dependencies": {
    "@ibootcloud/common-lib": "0.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "socket.io-client": "^4.3.2"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.173",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^16.10.1",
    "@types/socket.io-client": "^3.0.0",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
